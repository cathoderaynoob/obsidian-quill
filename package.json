{
  "name": "quill",
  "version": "1.0.1",
  "description": "Quill is your AI assistant and custom command creator.",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}' --quiet"
  },
  "prettier": {
    "useTabs": false
  },
  "keywords": [
    "obsidian",
    "plugin",
    "gpt",
    "openai",
    "text generation"
  ],
  "author": "Paul Nauman",
  "email": "cathoderaynoob@gmail.com",
  "repository": "https://github.com/cathoderaynoob/obsidian-quill",
  "license": "MIT",
  "devDependencies": {
    "@babel/eslint-parser": "^7.27.0",
    "@types/node": "^22.14.1",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@typescript-eslint/eslint-plugin": "8.30.1",
    "@typescript-eslint/parser": "8.30.1",
    "builtin-modules": "5.0.0",
    "esbuild": "0.25.2",
    "eslint-plugin-react": "^7.37.5",
    "obsidian": "latest",
    "tslib": "2.8.1",
    "typescript": "5.8.3"
  },
  "dependencies": {
    "date-fns": "^4.1.0",
    "lucide-react": "^0.488.0",
    "openai": "^4.95.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-markdown": "^10.1.0"
  }
}
